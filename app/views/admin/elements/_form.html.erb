<%= semantic_form_for [:admin, @element], builder: ActiveAdmin::FormBuilder do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :file, as: :file %>
    <%= f.input :tag_list, label: "Tags (separated by comma)", input_html: { value: f.object.tag_list.join(", ") } %>
    <%= f.actions %>
  <% end %>
<% end %>
